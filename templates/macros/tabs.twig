{% macro render(tabs, href) %}
    <ul class="uk-dark uk-subnav uk-subnav-divider" uk-margin>
    {% for tab in tabs %}
        <li{{ tab.href == href ? ' class="uk-active"' : '' }}>
            <a href="{{ tab.href }}">
                {{ tab.title }}
            </a>
        </li>
    {% endfor %}
    </ul>
{% endmacro %}
